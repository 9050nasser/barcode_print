 "css": "* {\n    font-family: 'Cairo', sans-serif;\n    font-size:10px;\n}\n.main {\n    max-height:25mm;\n    max-width:50mm;\n    align-items: center;\n    text-align: center;\n    border: 1px solid black;\n    margin-top: 5px;\n}\n.company {\n    text-align: center;\n    font-size:12px;\n}\n.barcode_header {\n    display: flex;\n    text-align: center;\n}\n.item_code {\n    margin: auto;\n}\n.item_name {\n    margin: auto;\n}\n.item_barcode {\n    text-align:center;\n}\nsvg, .barcode {\n    width: 50mm;\n    \n}\n.price {\n    margin-right: 100%;\n    margin-right: 35mm;\n    direction: ltr;\n}\n",
